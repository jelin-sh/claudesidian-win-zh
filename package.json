{
  "name": "claudesidian-win-zh",
  "version": "0.14.2",
  "description": "Claude Code + Obsidian 入门工具包 - AI 驱动的第二大脑 (Windows 中文版)",
  "type": "module",
  "scripts": {
    "setup": "pnpm install && echo '✅ 安装完成! 请配置 GEMINI_API_KEY 以启用视觉功能'",
    "test-gemini": "node .claude/mcp-servers/gemini-vision.mjs",
    "lint": "eslint --config .config/eslint.config.js . --fix && prettier --config .config/.prettierrc.js --write .",
    "lint:check": "eslint --config .config/eslint.config.js . && prettier --config .config/.prettierrc.js --check .",
    "format": "prettier --config .config/.prettierrc.js --write .",
    "format:check": "prettier --config .config/.prettierrc.js --check .",
    "attachments:list": "node .scripts/attachments-list.mjs",
    "attachments:count": "node .scripts/attachments-count.mjs",
    "attachments:organized": "node .scripts/attachments-organized.mjs",
    "attachments:unprocessed": "pnpm attachments:count",
    "attachments:refs": "node .scripts/attachments-refs.mjs",
    "attachments:sizes": "node .scripts/attachments-sizes.mjs",
    "attachments:orphans": "node .scripts/attachments-orphans.mjs",
    "attachments:recent": "node .scripts/attachments-recent.mjs",
    "attachments:create-organized": "node .scripts/attachments-create-organized.mjs",
    "transcript:extract": "node .scripts/transcript-extract.mjs",
    "vault:stats": "node .scripts/vault-stats.mjs",
    "check-updates": "node .scripts/check-updates.mjs",
    "firecrawl:scrape": "node .scripts/firecrawl-scrape.mjs",
    "firecrawl:batch": "node .scripts/firecrawl-batch.mjs"
  },
  "keywords": [
    "obsidian",
    "claude",
    "claude-code",
    "ai",
    "second-brain",
    "knowledge-management",
    "para-method"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@google/generative-ai": "^0.21.0",
    "@modelcontextprotocol/sdk": "^1.25.2"
  },
  "devDependencies": {
    "@eslint/compat": "^1.4.1",
    "@typescript-eslint/eslint-plugin": "^8.53.0",
    "@typescript-eslint/parser": "^8.53.0",
    "eslint": "^9.39.2",
    "eslint-plugin-check-file": "^3.3.1",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-perfectionist": "^4.15.1",
    "globals": "^16.5.0",
    "prettier": "^3.7.4",
    "prettier-plugin-packagejson": "^2.5.21",
    "prettier-plugin-sort-json": "^4.2.0",
    "typescript-eslint": "^8.53.0"
  },
  "packageManager": "pnpm@9.15.4"
}
